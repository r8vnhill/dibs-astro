{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "src",
    "astro.config.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$layouts/*": [
        "src/layouts/*"
      ],
      "$styles/*": [
        "src/styles/*"
      ],
      "$utils/*": [
        "src/utils/*"
      ],
      "$hooks/*": [
        "src/hooks/*"
      ],
      "$assets/*": [
        "src/assets/*"
      ],
      "$icons": [
        "src/assets/img/icons/index.ts"
      ],
      "$semantics": [
        "$components/semantics/index.ts"
      ]
    },
    "types": [
      "astro/client"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true
  }
}