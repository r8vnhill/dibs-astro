---
import * as Icons from "~/assets/img/icons";
import ShellBlock from "../ShellBlock.astro";

const { code, class: className = "" } = Astro.props;
---

<ShellBlock
    lang="bash" icon={Icons.Bash}
    {code} class={className}
>
    <span slot="title">
        <slot name="title" />
    </span>
</ShellBlock>
